{
    "env": {
        "browser": false,
        "commonjs": false,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [

    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "func-names": "off",
        "no-underscore-dangle": "warn",
        "no-console": "off",
        "camelcase": "warn",
        "import/extensions": "off",
        "radix": "off"
    }
}
